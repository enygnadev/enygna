{
  "name": "enygna-enterprises",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "22.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev -p 5000 -H 0.0.0.0",
    "build": "next build",
    "start": "next start -p 5000 -H 0.0.0.0",
    "lint": "next lint",
    "deploy": "vercel --prod",
    "emu": "firebase emulators:start --import ./.emu --export-on-exit",
    "emu:start": "firebase emulators:start",
    "emu:dev": "concurrently \"npm run emu\" \"npm run dev\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:emu": "cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 AUTH_EMULATOR_HOST=127.0.0.1:9099 jest",
    "seed": "ts-node scripts/seed.ts",
    "seed:emu": "cross-env FIRESTORE_EMULATOR_HOST=127.0.0.1:8080 AUTH_EMULATOR_HOST=127.0.0.1:9099 ts-node scripts/seed.ts"
  },
  "dependencies": {
    "@firebase/rules-unit-testing": "^5.0.0",
    "@google/generative-ai": "^0.24.1",
    "@types/jest": "^30.0.0",
    "@types/leaflet": "^1.9.20",
    "@types/papaparse": "^5.3.16",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "date-fns": "^3.6.0",
    "firebase": "^12.1.0",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "framer-motion": "^12.23.12",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "leaflet": "^1.9.4",
    "next": "^15.4.6",
    "next-auth": "^4.24.11",
    "openai": "^4.0.0",
    "papaparse": "^5.5.3",
    "postcss": "^8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-leaflet": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.4.6",
    "fast-glob": "^3.3.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.9.2"
  }
}
